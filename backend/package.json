{
  "name": "stock-analysis-backend",
  "version": "1.0.0",
  "description": "Backend services for stock analysis application",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "build": "echo 'No build step needed for Node.js'",
    "test": "jest",
    "start:agents": "concurrently \"npm run start:stock-agent\" \"npm run start:news-agent\" \"npm run start:economic-agent\" \"npm run start:analysis-agent\" \"npm run start:ui-agent\"",
    "start:stock-agent": "node src/agents/stockDataAgent.js",
    "start:news-agent": "node src/agents/newsSentimentAgent.js",
    "start:economic-agent": "node src/agents/economicIndicatorAgent.js",
    "start:analysis-agent": "node src/agents/analysisAgent.js",
    "start:ui-agent": "node src/agents/uiAgent.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.4",
    "redis": "^4.6.10",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1",
    "node-cron": "^3.0.3",
    "sentiment": "^5.0.2",
    "technicalindicators": "^3.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "winston": "^3.11.0",
    "joi": "^17.11.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  }
} 