# Copy this file to .env and replace with your actual API keys

# API Keys - Replace with your actual API keys
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here
FINNHUB_API_KEY=your_finnhub_api_key_here
TWELVE_DATA_API_KEY=your_twelve_data_api_key_here
NEWS_API_KEY=your_news_api_key_here
NEWSDATA_API_KEY=your_newsdata_api_key_here
WEBZ_API_KEY=your_webz_api_key_here
FRED_API_KEY=your_fred_api_key_here
STOCKTWITS_ACCESS_TOKEN=your_stocktwits_access_token_here
TWITTER_BEARER_TOKEN=your_twitter_bearer_token_here
TWINWORD_API_KEY=your_twinword_api_key_here

# Server Configuration
PORT=3001
NODE_ENV=development
CORS_ORIGIN=http://localhost:3000

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Database Configuration (if using MongoDB)
MONGODB_URI=mongodb://localhost:27017/stock_analysis

# Logging
LOG_LEVEL=info

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Cache Configuration
CACHE_TTL_SECONDS=300
STOCK_DATA_CACHE_TTL=60
NEWS_CACHE_TTL=1800
ECONOMIC_DATA_CACHE_TTL=3600

# Analysis Configuration
CONFIDENCE_THRESHOLD=0.6
SENTIMENT_WEIGHT=0.3
TECHNICAL_WEIGHT=0.4
ECONOMIC_WEIGHT=0.3

# Ollama Configuration
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama4:maverick
OLLAMA_SENTIMENT_MODEL=llama4:maverick
OLLAMA_ANALYSIS_MODEL=llama4:maverick
OLLAMA_TECHNICAL_MODEL=llama4:maverick
OLLAMA_TIMEOUT=30000
OLLAMA_MAX_RETRIES=3
OLLAMA_ENABLED=true

# Data Source Configuration
USE_MOCK_DATA=false 